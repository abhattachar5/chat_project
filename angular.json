{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "insurance-chat-widget": {
      "projectType": "library",
      "root": "projects/insurance-chat-widget",
      "sourceRoot": "projects/insurance-chat-widget/src",
      "prefix": "ins",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/insurance-chat-widget/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/insurance-chat-widget/tsconfig.lib.prod.json",
              "optimization": true,
              "sourceMap": false,
              "declarationMap": false
            },
            "development": {
              "tsConfig": "projects/insurance-chat-widget/tsconfig.lib.json",
              "optimization": false,
              "sourceMap": true,
              "declarationMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/insurance-chat-widget/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "karmaConfig": "karma.conf.js",
            "styles": [
              "projects/insurance-chat-widget/src/styles.scss"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/insurance-chat-widget/**/*.ts",
              "projects/insurance-chat-widget/**/*.html"
            ]
          }
        }
      },
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      }
    },
    "insurance-chat-widget-demo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/insurance-chat-widget-demo",
      "sourceRoot": "projects/insurance-chat-widget-demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/insurance-chat-widget-demo",
            "index": "projects/insurance-chat-widget-demo/src/index.html",
            "browser": "projects/insurance-chat-widget-demo/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/insurance-chat-widget-demo/tsconfig.app.json",
            "assets": [
              "projects/insurance-chat-widget-demo/src/favicon.ico",
              "projects/insurance-chat-widget-demo/src/assets"
            ],
            "styles": [
              "projects/insurance-chat-widget-demo/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "insurance-chat-widget-demo:build:production"
            },
            "development": {
              "buildTarget": "insurance-chat-widget-demo:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}

